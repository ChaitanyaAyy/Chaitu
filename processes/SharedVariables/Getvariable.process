        <pd:resourceType>ae.activities.FileReadActivity</pd:resourceType>
        <pd:x>262</pd:x>
        <pd:y>56</pd:y>
        <config>
            <encoding>text</encoding>
        </config>
        <pd:inputBindings>
            <ns:ReadActivityInputClass>
                <fileName>
                    <xsl:value-of select="&quot;E:\tibco\Training\inputFile\sampleRead.txt&quot;"/>
                </fileName>
            </ns:ReadActivityInputClass>
        </pd:inputBindings>
