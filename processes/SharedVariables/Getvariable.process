    <pd:endName>End</pd:endName>
    <pd:endX>637</pd:endX>
    <pd:endY>59</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1623643937927</pd:targetNamespace>
    <pd:activity name="GetVariable">
        <pd:type>com.tibco.pe.core.GetSharedVariableActivity</pd:type>
        <pd:resourceType>ae.activities.getSharedVariable</pd:resourceType>
        <pd:x>409</pd:x>
        <pd:y>59</pd:y>
        <config>
